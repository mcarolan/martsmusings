      &lt;ul&gt;
        &lt;li&gt;&lt;a href=&#34;#kaya-toast&#34;&gt;Kaya Toast&lt;/a&gt;&lt;/li&gt;
        &lt;li&gt;&lt;a href=&#34;#singapore-sling&#34;&gt;Singapore Sling&lt;/a&gt;&lt;/li&gt;
        &lt;li&gt;&lt;a href=&#34;#fried-carrot-cake&#34;&gt;Fried Carrot Cake&lt;/a&gt;&lt;/li&gt;
        &lt;li&gt;&lt;a href=&#34;#gobi-manchurian-and-dosa&#34;&gt;Gobi Manchurian and Dosa&lt;/a&gt;&lt;/li&gt;
      &lt;/ul&gt;
    &lt;/li&gt;
    &lt;li&gt;&lt;a href=&#34;#weird-and-wonderful&#34;&gt;Weird and wonderful&lt;/a&gt;&lt;/li&gt;
